animaux = ["giraf", "tigre", "singe", "souris"]
tailles = [5, 2.5, 1.75, 0.15]
mixte = ["giraf", 5, "souris", 0.15]
print (animaux + tailles + mixte)


print (animaux[2], tailles[1])


ani1 = ["singe", "souris"]
ani2 = ["tigre", "singe"]
print (ani1 + ani2)

